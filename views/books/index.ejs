<% include ../partials/header.ejs %>

<h1>Books</h1>

<table class="table table-striped table-hover">
    <tr><th>Title</th><th>Author</th><th>Price</th><th>Year</th></tr>

    <% for (var i = 0; i < books.length; i++) { %>
        <tr>
            <td><%= books[i].title %></td>
            <td><%= books[i].author %></td>
            <td><%= books[i].price %></td>
            <td><%= books[i].year %></td>
        </tr>
    <% } %>

</table>

